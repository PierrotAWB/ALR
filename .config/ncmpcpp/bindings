def_key "U"
	update_database

def_key "j"
	scroll_down

def_key "k"
	scroll_up

def_key "h"
	jump_to_parent_directory

def_key "l"
	enter_directory

def_key "l"
	run_action

def_key "l"
	play_item

def_key "l"
	next_column

def_key "h"
	previous_column

def_key "n"
	next_found_item

def_key "N"
	previous_found_item

def_key "f"
	show_browser

def_key "p"
	show_playlist

def_key "P"
	pause

def_key "x"
	delete_playlist_items

def_key "="
	volume_up

def_key "ctrl-f"
	seek_forward

def_key "ctrl-b"
	seek_backward

def_key "t"
	show_clock
